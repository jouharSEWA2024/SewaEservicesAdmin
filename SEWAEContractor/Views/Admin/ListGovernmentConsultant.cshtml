@model SEWAEContractorAdmin.Models.PremiseDetailsForGovernmentViewModel
@{
    ViewData["Title"] = "ManageConsultant";
    @using Microsoft.AspNetCore.Http.Features
    @using Microsoft.AspNetCore.Mvc.Localization;
    @using Microsoft.AspNetCore.Localization;
    @inject IViewLocalizer localizer;
    string culture = "", uiculture = "", url = "";
    if (Context.Request.QueryString.HasValue)
    {
        url = Context.Request.QueryString.Value;
        culture = Context.Request.Query["culture"];
        uiculture = Context.Request.Query["ui-culture"];
    }
    var requestCulture = Context.Features.Get<IRequestCultureFeature>();
}

<div class=" content-body default-height default-height">
    <div class="container-fluid">
        
        <!-- row -->
        <div class="row">
            <div class="col-xl-12 col-xxl-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">@localizer["Consultant/Contractor Info"]</h4>
                    </div>
                    <div class="card-body">
                        
                        <br />
                        @if (Model.consultantPremiseDetailsForGovtUserOutputList != null && Model.consultantPremiseDetailsForGovtUserOutputList.Count > 0)
                        {
                            var j = 1;
                            <div class="table-responsive">
                                <table id="example3" class="display  responsive dt-responsive" style="width:100%;">
                                    <thead>
                                        <tr>
                                            <th data-priority="1">@localizer["Serial No"]</th>
                                            <th>@localizer["Government Entity Name"]</th>
                                            <th>@localizer["Company Name"]</th>
                                            <th data-priority="2">@localizer["City"]</th>
                                            <th>@localizer["Area"]</th>
                                            <th>@localizer["Plot"]</th>
                                            <th>@localizer["Project Completion Date"]</th>
                                            
                                        </tr>
                                    </thead>
                                    <tbody>

                                        @foreach (var premise in Model.consultantPremiseDetailsForGovtUserOutputList)
                                        {

                                            <tr>
                                                <td>@j</td>
                                                <td>@premise.EntityBPName</td>
                                                <td>@premise.ApprovedConsultantBPName</td>
                                                <td>@premise.CityName</td>
                                                <td>@premise.Area</td>
                                                <td>@premise.Plot</td>
                                                <td>@premise.ConsultantExpiryDate.ToString("dd/MM/yyyy")</td>
                                            </tr>
                                            j++;
                                        }
                                    </tbody>
                                </table>
                            </div>
                        }

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--**********************************
    Content body end
***********************************-->
